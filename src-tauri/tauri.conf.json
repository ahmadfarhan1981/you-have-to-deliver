{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "you-need-to-deliver",
  "version": "0.1.0",
  "identifier": "com.ahmadfarhan.you-need-to-deliver",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "you-need-to-deliver",
        "center": true,
        "minWidth": 900,
        "minHeight": 700,
        "width": 900,
        "height": 700
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
